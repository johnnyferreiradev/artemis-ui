{
  "name": "@johnnyferreiradev/artmis-ui",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "publishConfig": {
    "@johnnyferreiradev:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "shx rm -rf dist && tsc && vite build",
    "prepack": "yarn build",
    "postpack": "shx rm -rf src public",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/artemis.es.js",
      "require": "./dist/artemis.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "site.zip/path/to/entry/point.js",
  "module": "./dist/artemis.es.js",
  "types": "./dist/index.d.ts",
