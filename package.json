    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "keywords": [],
  "author": "Johnny Ferreira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnnyferreiradev/nemea-ui"
  },
  "dependencies": {
